{
  "name": "gatsby-personal-starter",
  "private": true,
  "description": "A gatsby starter using ReasonML, postcss, and tailwind.",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H '0.0.0.0'",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "dev:reason": "bsb -clean-world -make-world -w"
  },
  "dependencies": {
    "bs-date-fns": "^0.1.6",
    "bs-platform": "^8.2.0",
    "gatsby": "^2.24.47",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-purgecss": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reason-react": "^0.9.1",
    "tailwindcss": "^1.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  }
}
